{
  "compilerOptions": {
    "target": "esnext",                         
    "jsx": "react",                           
    "noEmit": true,                           
    "strict": true,                           
    "moduleResolution": "node",               
    "esModuleInterop": true,                  
    "skipLibCheck": true,                     
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}

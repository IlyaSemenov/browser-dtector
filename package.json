{
	"name": "browser-dtector",
	"version": "1.0.0",
	"description": "A Javascript library to detect browser, version and platform",
	"author": "sibiraj-s",
	"license": "MIT",
	"private": true,
	"repository": "https://github.com/sibiraj-s/browser-dtector.git",
	"homepage": "https://github.com/sibiraj-s/browser-dtector#readme",
	"bugs": "https://github.com/sibiraj-s/browser-dtector/issues",
	"engines": {
		"node": "^10"
	},
	"scripts": {
		"rollup": "rollup -c",
		"develop": "rollup -cw",
		"terser": "terser dist/lib/browser-dtector.js -o dist/lib/browser-dtector.min.js --source-map  --comments \"/\\*!\"",
		"build:lib": "npm run rollup && npm run terser",
		"build:docs": "parcel build docs/index.html --out-dir dist/docs --public-url /browser-dtector/",
		"build": "npm run build:lib && npm run build:docs",
		"postbuild": "sh ./scripts/postbuild.sh",
		"prebuild": "sh ./scripts/prebuild.sh",
		"serve:docs": "parcel docs/index.html --out-dir dist/docs",
		"test": "jest",
		"test:ci": "npm run build && npm run test",
		"lint:es": "eslint . --ignore-path .gitignore",
		"lint:ts": "tslint -p tsconfig.json",
		"lint": "npm run lint:es && npm run lint:ts",
		"prepublishOnly": "node ./scripts/prepublish.js",
		"prepublish:docs": "npm run build:docs",
		"preupdate:docs": "npm run build:docs",
		"publish:docs": "node ./scripts/publish-docs.js --release",
		"update:docs": "node ./scripts/publish-docs.js --update",
		"prerelease": "npm run build",
		"release": "sh ./scripts/release.sh"
	},
	"keywords": [
		"browser",
		"detector",
		"browser-dtector",
		"browser-detector",
		"useragent",
		"useragent-parser",
		"nodejs"
	],
	"dependencies": {
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"react-router-dom": "^4.3.1"
	},
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@babel/preset-env": "^7.3.1",
		"@babel/preset-react": "^7.0.0",
		"babel-jest": "^24.0.0",
		"chalk": "^2.4.2",
		"eslint": "^5.12.1",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-config-airbnb-base": "^13.1.0",
		"eslint-plugin-import": "^2.15.0",
		"eslint-plugin-jsx-a11y": "^6.1.2",
		"eslint-plugin-react": "^7.12.4",
		"gh-pages": "^2.0.1",
		"husky": "^1.3.1",
		"identity-obj-proxy": "^3.0.0",
		"inquirer": "^6.2.1",
		"jest": "^24.0.0",
		"nodemon": "^1.18.9",
		"parcel-bundler": "^1.11.0",
		"react-test-renderer": "^16.7.0",
		"rollup": "^1.1.2",
		"rollup-plugin-babel": "^4.3.2",
		"rollup-plugin-json": "^3.1.0",
		"rollup-plugin-typescript": "^1.0.0",
		"sass": "^1.16.1",
		"terser": "^3.14.1",
		"tslib": "^1.9.3",
		"tslint": "^5.12.1",
		"typescript": "^3.2.4",
		"yargs-parser": "^11.1.1"
	}
}

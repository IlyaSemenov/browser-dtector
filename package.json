{
  "name": "browser-dtector",
  "version": "1.0.1",
  "description": "A Javascript library to detect browser, version and platform",
  "author": "sibiraj-s",
  "license": "MIT",
  "private": true,
  "repository": "https://github.com/sibiraj-s/browser-dtector.git",
  "homepage": "https://github.com/sibiraj-s/browser-dtector#readme",
  "bugs": "https://github.com/sibiraj-s/browser-dtector/issues",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "rollup": "rollup -c",
    "terser": "terser dist/lib/browser-dtector.js -o dist/lib/browser-dtector.min.js --source-map  --comments \"/\\*!\"",
    "build:lib": "npm run rollup && npm run terser",
    "build:docs": "parcel build docs/index.html --out-dir dist/docs --public-url /browser-dtector/",
    "build": "npm run build:lib && npm run build:docs",
    "postbuild": "sh ./scripts/postbuild.sh",
    "prebuild": "sh ./scripts/prebuild.sh",
    "serve:docs": "parcel docs/index.html --out-dir dist/docs",
    "test": "jest",
    "lint": "eslint . --ignore-path .gitignore",
    "prepublishOnly": "node ./scripts/prepublish.js",
    "prepublish:docs": "npm run build:docs",
    "preupdate:docs": "npm run build:docs",
    "publish:docs": "node ./scripts/publish-docs.js --release",
    "update:docs": "node ./scripts/publish-docs.js --update",
    "prerelease": "npm run build",
    "release": "sh ./scripts/release.sh"
  },
  "keywords": [
    "browser",
    "detector",
    "browser-dtector",
    "browser-detector",
    "useragent",
    "useragent-parser",
    "nodejs"
  ],
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel-jest": "^24.9.0",
    "chalk": "^3.0.0",
    "eslint": "^6.7.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^7.0.0",
    "jest": "^24.9.0",
    "parcel-bundler": "^1.12.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "rollup": "^1.27.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-json": "^4.0.0",
    "sass": "^1.23.7",
    "terser": "^4.4.0",
    "yargs-parser": "^16.1.0"
  }
}

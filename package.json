{
  "name": "browser-dtector",
  "version": "2.1.0",
  "description": "A Javascript library to detect browser, version and platform",
  "author": "sibiraj-s",
  "license": "MIT",
  "private": true,
  "repository": "https://github.com/sibiraj-s/browser-dtector.git",
  "homepage": "https://github.com/sibiraj-s/browser-dtector#readme",
  "bugs": "https://github.com/sibiraj-s/browser-dtector/issues",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "gulp build",
    "serve": "parcel docs/index.html --out-dir dist/docs",
    "test": "jest",
    "lint": "eslint . --ignore-path .gitignore",
    "prepublishOnly": "node ./scripts/prepublish.js",
    "publish:docs": "gulp buildDocs && node ./scripts/publish-docs.js --release",
    "update:docs": "gulp buildDocs && node ./scripts/publish-docs.js --update",
    "prerelease": "npm run build",
    "release": "sh ./scripts/release.sh"
  },
  "keywords": [
    "browser",
    "detector",
    "browser-dtector",
    "browser-detector",
    "useragent",
    "useragent-parser",
    "nodejs"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-jest": "^25.1.0",
    "chalk": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "gh-pages": "^2.2.0",
    "gulp": "^4.0.2",
    "gulp-plugin-terser": "^1.1.1",
    "gulp-sourcemaps": "^2.6.5",
    "husky": "^4.2.3",
    "inquirer": "^7.1.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.9",
    "parcel-bundler": "^1.12.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "rollup": "^2.2.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-json": "^4.0.0",
    "sass": "^1.26.3",
    "terser": "^4.6.7",
    "through2": "^3.0.1",
    "yargs-parser": "^18.1.1"
  }
}
